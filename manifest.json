{
    "name": "Infinity Accessibility",
    "description": "extensão protótipo",
    "version": "0.0.2",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_popup": "Extensao/extensao.html"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["js/modo_daltonico.js"]
        }
    ],
    "background": {
        "service_worker": "color-blind/color-blind/lib/color-blind.js"
    }
}
